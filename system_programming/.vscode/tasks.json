{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "shell",
      "label": "gcc build ext2_Parsing",
      "command": "gcc",
      "args": [
        "-ansi",
        "-pedantic-errors",
        "-Wall",
        "-Wextra",
        "-g",
        "-I${workspaceFolder}/include",
        "${workspaceFolder}/src/authentication.c",
        "${workspaceFolder}/test/authentication_test.c",
        "-o",
        "${workspaceFolder}/test/test.out",
        "-lcrypt",
        "-lpam",
        "-lpam_misc"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "type": "shell",
      "label": "gcc build ext2_Parsing_test",
      "command": "gcc",
      "args": [
        "-ansi",
        "-pedantic-errors",
        "-Wall",
        "-Wextra",
        "-g",
        "-I${workspaceFolder}/include",
        "${workspaceFolder}/src/ext2_Parsing.c",
        "${workspaceFolder}/test/ext2_Parsing_test.c",
        "-o",
        "${workspaceFolder}/test/ext2_Parsing_test.out",
        "-lcrypt",
        "-lpam",
        "-lpam_misc"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": [
        "$gcc"
      ]
    }
  ]
}